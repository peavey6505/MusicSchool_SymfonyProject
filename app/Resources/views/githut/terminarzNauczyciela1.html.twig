{% block stylesheets %}
    <link href="{{ asset('css/terminarzNauczyciela.css') }}" rel="stylesheet" />
{% endblock %}

{% block body %}

    <!DOCTYPE HTML>
    <html lang="pl">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <title> </title>
    </head>

    <body>

   <h1> Terminarz </h1> <br /> <br />

    <table border="1" cellpadding="10" cellpadding="0" >
        <tr>
            <td> Dzień </td> <td> Godzina </td> <td> Instrument </td><td> Uczeń </td>
        </tr>

        {% for result in results %}

            <tr>
                <td> {{ result.dzien }} </td> <td> {{ result.godzina }} </td> <td> {{ result.instrument }} </td> <td> {{ result.imienazwisko }} </td>

            </tr>
        {% endfor %}
    </table>

    <form  action={{path('profilNauczyciela')  }} method="post">
        <input type="hidden" name="id" value={{ id }}>
        <button type="submit">Powrót</button >
    </form>
    </body>


    </html>

{% endblock %}